{
  "name": "web-auto-extractor",
  "version": "1.0.1",
  "description": "Automatically extracts structured information from webpages",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "test": "mocha --recursive --require babel-core/register",
    "lint": "standard",
    "dev": "nodemon --exec npm run lint",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "engines": {
    "node": ">=4.4"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "cheerio": "^1.0.0-rc.10",
    "htmlparser2": "^6.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.3.4",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "sinon": "^11.1.1",
    "standard": "^16.0.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "tonicExample.js"
    ]
  },
  "keywords": [
    "crawler",
    "parser",
    "html parser",
    "schema.org",
    "microdata",
    "rdfa",
    "jsonld",
    "seo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/indix/web-auto-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/indix/web-auto-extractor/issues"
  },
  "tonicExampleFilename": "tonicExample.js"
}
